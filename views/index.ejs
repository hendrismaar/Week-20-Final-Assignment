<%- include('partials/header') %>   
    <link rel="stylesheet" href="css/index.css">
    <title>Final Project</title>
</head>
<body>
  <%- include('partials/navbar') %>

    <div class="container">
            <main>
                <ul>
                    <li><h2>Personal information</h2></li>
                        <li><%= displayInfo.infoContent %></li>
                </ul>
            </main> 

            <div id="education">
                <ul>
                    <li>
                        <h2>Education</h2>
                        <table>
                            <tr>
                                <th>School</th>
                                <th>Year of graduation</th>
                            </tr>
                                <tr>
                                    <td><%= displayInfo.eduSchoolContent %></td>
                                    <td><%= displayInfo.eduYearContent %></td>
                                </tr>
                    </table>
                    </li>
                </ul>
            </div>

            <div id="technical">
                <ul>
                    <li><h2>Technical skills</h2></li>
                        <li><%= displayInfo.techContent %></li>
                </ul>
            </div>

            <div id="soft">
                
                <ul>
                    <li><h2>Soft skills</h2></li>
                        <li><%= displayInfo.softContent %></li>
                </ul>
            </div>

            <div id="img">
                <ul>
                    <li><h2>Image</h2></li>
                    <li>
                        <img src="<%= displayInfo.imageUrl %>" alt=""> 
                    </li>
                </ul>
            </div>
        </div>
<%- include('partials/footer') %>
